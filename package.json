{
  "name": "ledap",
  "version": "0.1.10",
  "description": "a javascript framwork, which can collaboration with Vue, angular, jquery etc.",
  "main": "./dist/index.js",
  "scripts": {
    "init": "bash scripts/init.sh",
    "example": "cd examples/ && npm run start",
    "dev": "webpack --display-modules",
